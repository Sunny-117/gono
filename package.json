{
  "name": "rono",
  "version": "0.0.0",
  "packageManager": "pnpm",
  "description": "My awesome typescript library",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/Sunny-117/rono#readme",
  "bugs": {
    "url": "https://github.com/Sunny-117/rono/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sunny-117/rono.git"
  },
  "author": "Sunny-117",
  "funding": "https://github.com/sponsors/Sunny-117",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "rono": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./cli": {
      "import": "./dist/cli.js",
      "default": "./dist/cli.js"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint --cache .",
    "lint:fix": "pnpm run lint --fix",
    "build": "node ./scripts/build.mjs",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "format": "prettier --cache --write .",
    "release": "bumpp && pnpm publish",
    "prepublishOnly": "pnpm run build"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.4.1",
    "@types/node": "^20.16.3",
    "bumpp": "^9.5.2",
    "eslint": "^9.9.1",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  },
  "engines": {
    "node": ">=18.12.0"
  },
  "prettier": "@Sunny-117/prettier-config",
  "dependencies": {
    "@rolldown/binding-wasm32-wasi": "1.0.0-beta.41",
    "rolldown": "1.0.0-beta.41"
  }
}
