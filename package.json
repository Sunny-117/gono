{
  "name": "gono",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@10.9.0",
  "description": "⚡️ TypeScript Execute | The easiest way to run TypeScript in Node.js with Rolldown",
  "author": "Sunny-117",
  "license": "MIT",
  "funding": "https://github.com/sponsors/Sunny-117",
  "homepage": "https://github.com/Sunny-117/gono#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sunny-117/gono.git"
  },
  "bugs": {
    "url": "https://github.com/Sunny-117/gono/issues"
  },
  "keywords": [
    "typescript",
    "execute",
    "rolldown",
    "esno",
    "tsx"
  ],
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.mjs",
  "bin": {
    "gono": "./dist/cli.mjs"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.12.0"
  },
  "scripts": {
    "lint": "eslint --cache .",
    "lint:fix": "pnpm run lint --fix",
    "build": "robuild",
    "test": "vitest --run",
    "typecheck": "tsc --noEmit",
    "format": "prettier --cache --write .",
    "release": "pnpm test && changelogen --release && npm publish && git push --follow-tags",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "cac": "^6.7.14",
    "chokidar": "^5.0.0",
    "consola": "^3.4.2",
    "rolldown": "1.0.0-rc.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^7.4.3",
    "@types/node": "^25.2.3",
    "changelogen": "^0.6.2",
    "eslint": "^10.0.0",
    "prettier": "^3.8.1",
    "robuild": "^0.1.3",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
